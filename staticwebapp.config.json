{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/*.{ico,png}"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platform": {
    "apiRuntime": "node:16"
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
} 